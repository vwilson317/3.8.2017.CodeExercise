<!DOCTYPE html>
<html ng-app="app" ng-controller="AppController as vm" ng-cloak>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Title Search App</title>
    <link rel="SHORTCUT ICON" href="~/favicon.ico" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.min.js"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/libs")
    @Scripts.Render("~/bundles/app")
</head>
<body layout="column">
    @Styles.Render("~/Content/css")
    <md-toolbar layout="row">
        <h1 class="md-padding site-title">
            Title Search
        </h1>
    </md-toolbar>
<md-ink-bar></md-ink-bar>
    <!-- Container #2 -->
    <div id="main-content" flex layout="column" layout-align="stretch">
        
        <!-- Container #4 -->
        @RenderBody()
    </div>
</body>
</html>